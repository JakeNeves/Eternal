<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\references\tModLoader.targets" />
  <PropertyGroup>
    <AssemblyName>Eternal</AssemblyName>
    <TargetFramework>net45</TargetFramework>
    <PlatformTarget>AnyCPU</PlatformTarget>
    <LangVersion>7.3</LangVersion>
  </PropertyGroup>
  <Target Name="BuildMod" AfterTargets="Build">
    <Exec Command="&quot;$(tMLBuildServerPath)&quot; -build $(ProjectDir) -eac $(TargetPath) -define $(DefineConstants) -unsafe $(AllowUnsafeBlocks)" />
  </Target>
  <ItemGroup>
    <PackageReference Include="tModLoader.CodeAssist" Version="0.1.*" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="CalamityMod">
      <HintPath>..bin\debug\net45\CalamityMod.XNA.dll</HintPath>
    </Reference>
    <Reference Include="EternalMusic">
      <HintPath>..\..\references\mods\EternalMusic.XNA.dll</HintPath>
    </Reference>
    <Reference Include="FargowiltasSouls">
      <HintPath>..bin\debug\net45\FargowiltasSouls.XNA.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>